#include <bits/stdc++.h>

using namespace std;

int main()
{
   int t;
   cin>>t;
   while(t--)
   {
       int n,k;
       cin>>n>>k;
       map<int,vector<int>> m;
       int a[n],res[n]={0};
       for(int i=0;i<n;i++)
       {
            cin>>a[i];
            m[a[i]].push_back(i);
       }
       for(auto it=m.begin();it!=m.end();it++)
       {
           cout<<it->first<<"-->";
           for(int i=0;i<it->second.size();i++)
           {
               cout<<it->second[i];
           }
           cout<<"\n";
       }
      int t=0;
      for(auto it=m.begin();it!=m.end();it++)
      {
          for(int i=0;i<it->second.size()&&i<k;i++)
          {
              res[it->second[i]]=++t;
              if(t>=k)
                  t%=k;
          }
      }
      for(int i=0;i<n;i++)
            cout<<res[i]<<" ";
        cout<<"\n";
   }
}

